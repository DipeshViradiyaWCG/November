<h1>Users &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<a id="exportBtn" class="btn btn-primary">Export to csv file</a></h1>
<br>
<table class="table" id="table">
    <thead>
        <tr>
            <th scope="col"> Name </th>
            <th scope="col"> Email </th>
            <th scope="col"> Contact </th>
        </tr>
    </thead>
    <tbody>
        {{#each users}}
            <tr>
                <td>{{name}}</td>
                <td>{{email}}</td>
                <td>{{contact}}</td>
            </tr>
        {{/each}}
    </tbody>
</table>
<br><br><br>
<a id="logoutBtn" class="btn btn-primary">Logout</a>